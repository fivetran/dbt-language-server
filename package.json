{
  "name": "dbt-language-server",
  "displayName": "dbt Language Server",
  "description": "This extension will help you work with dbt",
  "version": "0.0.3",
  "publisher": "Fivetran",
  "license": "MIT",
  "preview": true,
  "keywords": [
    "dbt",
    "bigquery",
    "sql"
  ],
  "extensionDependencies": [
    "ms-python.python"
  ],
  "engines": {
    "vscode": "^1.61.0"
  },
  "activationEvents": [
    "workspaceContains:**/dbt_project.yml"
  ],
  "main": "./client/out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Configuration",
      "properties": {
        "dbtLanguageServer.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    },
    "commands": [
      {
        "command": "editor.showQueryPreview",
        "title": "Show query preview"
      },
      {
        "command": "dbt.compile",
        "title": "dbt compile"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "dbt.compile",
          "group": "navigation",
          "when": "editorLangId =~ /sql$/"
        }
      ],
      "editor/context": [
        {
          "command": "editor.showQueryPreview",
          "group": "navigation@1.31",
          "when": "editorLangId =~ /sql$/"
        },
        {
          "command": "dbt.compile",
          "group": "navigation@1.31",
          "when": "editorLangId =~ /sql$/"
        }
      ]
    }
  },
  "aiKey": "0a32611a-7944-4c4d-93c9-5c260a61395b",
  "scripts": {
    "vscode:prepublish": "npm run webpack && npm run prepare-node-modules",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "format": "prettier --write \"client/src/**/*.ts\" \"server/src/**/*.ts\" \"markdown_parser/src/**/*.ts\" \"e2e/src/**/*.ts\"",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ../markdown_parser && npm install && cd ../e2e && npm install && cd ..",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "test": "mocha \"server/out/test/**/*.spec.js\"",
    "clean": "rm -rf client/out && rm -rf server/out",
    "prepare-zetasql": "cp -R ./server/node_modules/@fivetrandevelopers/zetasql/lib/protos/ ./server/out/protos/ && cp -R ./server/node_modules/@fivetrandevelopers/zetasql/lib/zetasql/ ./server/out/zetasql/",
    "webpack": "npm run clean && webpack --mode production --config ./client/webpack.config.js && webpack --mode production --config ./server/webpack.config.js && npm run prepare-zetasql",
    "webpack:dev": "npm run clean && webpack --mode none --config ./client/webpack.config.js && webpack --mode none --config ./server/webpack.config.js && npm run prepare-zetasql",
    "prepare-node-modules": "./prepare_node_modules.sh",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "merge-options": "^3.0.4",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "ts-loader": "^9.2.6",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache",
      "prettier --write"
    ]
  }
}
