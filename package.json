{
  "name": "dbt-language-server",
  "displayName": "dbt Wizard",
  "description": "This extension will help you work with dbt",
  "icon": "images/Icon.png",
  "version": "0.4.1",
  "publisher": "Fivetran",
  "license": "MIT",
  "preview": true,
  "keywords": [
    "dbt",
    "bigquery",
    "sql"
  ],
  "homepage": "https://github.com/fivetran/dbt-language-server/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/fivetran/dbt-language-server.git"
  },
  "bugs": {
    "url": "https://github.com/fivetran/dbt-language-server/issues",
    "email": "pavel@fivetran.com"
  },
  "extensionDependencies": [
    "ms-python.python"
  ],
  "engines": {
    "vscode": "^1.61.0"
  },
  "activationEvents": [
    "workspaceContains:**/dbt_project.yml"
  ],
  "main": "./client/out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Configuration",
      "properties": {
        "dbtLanguageServer.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    },
    "commands": [
      {
        "command": "editor.showQueryPreview",
        "title": "Show query preview"
      },
      {
        "command": "dbt.compile",
        "title": "dbt compile"
      },
      {
        "command": "dbt.refToSql",
        "title": "Convert ref to SQL"
      },
      {
        "command": "dbt.sqlToRef",
        "title": "Convert SQL to ref"
      }
    ],
    "menus": {
      "dbt": [
        {
          "command": "dbt.compile",
          "group": "1_common"
        },
        {
          "command": "editor.showQueryPreview",
          "group": "1_common"
        },
        {
          "command": "dbt.refToSql",
          "group": "2_ref_sql"
        },
        {
          "command": "dbt.sqlToRef",
          "group": "2_ref_sql"
        }
      ],
      "editor/context": [
        {
          "submenu": "dbt",
          "group": "navigation",
          "when": "editorLangId =~ /^sql$|^jinja-sql$|^sql-bigquery$/ && !editorReadonly"
        }
      ]
    },
    "submenus": [
      {
        "id": "dbt",
        "label": "dbt Wizard"
      }
    ]
  },
  "aiKey": "0a32611a-7944-4c4d-93c9-5c260a61395b",
  "size-limit": [
    {
      "path": "client/out/**/*"
    },
    {
      "path": "server/out/**/*.js"
    }
  ],
  "scripts": {
    "vscode:prepublish": "npm run webpack && ./prepare_node_modules.sh",
    "build": "tsc -b",
    "build:size": "npm run build && npm run vscode:prepublish",
    "watch": "tsc -b -w",
    "format": "prettier --write \"client/src/**/*.ts\" \"server/src/**/*.ts\" \"markdown_parser/src/**/*.ts\" \"e2e/src/**/*.ts\"",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ../markdown_parser && npm install && cd ../e2e && npm install && cd ..",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "test": "mocha",
    "clean": "rm -rf client/out && rm -rf server/out",
    "clean-node-modules": "rm -rf node_modules && rm -rf client/node_modules && rm -rf server/node_modules",
    "clean-install": "npm run clean && npm run clean-node-modules && npm ci",
    "prepare-zetasql": "cp -R ./server/node_modules/@fivetrandevelopers/zetasql/lib/protos/ ./server/out/protos/ && cp -R ./server/node_modules/@fivetrandevelopers/zetasql/lib/zetasql/ ./server/out/zetasql/",
    "webpack": "npm run clean && webpack --mode production --config ./client/webpack.config.js && webpack --mode production --config ./server/webpack.config.js && npm run prepare-zetasql",
    "webpack:dev": "npm run clean && webpack --mode none --config ./client/webpack.config.js && webpack --mode none --config ./server/webpack.config.js && npm run prepare-zetasql",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@size-limit/file": "^7.0.8",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.14.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "merge-options": "^3.0.4",
    "mocha": "^9.2.2",
    "prettier": "^2.6.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "size-limit": "^7.0.8",
    "ts-loader": "^9.2.8",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache",
      "prettier --write"
    ]
  }
}
