{
  "name": "dbt-language-server",
  "displayName": "dbt Wizard",
  "description": "This extension will help you work with dbt",
  "icon": "images/Icon.png",
  "version": "0.11.1",
  "publisher": "Fivetran",
  "license": "MIT",
  "preview": true,
  "keywords": [
    "dbt",
    "bigquery",
    "sql"
  ],
  "homepage": "https://github.com/fivetran/dbt-language-server/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/fivetran/dbt-language-server.git"
  },
  "bugs": {
    "url": "https://github.com/fivetran/dbt-language-server/issues",
    "email": "pavel@fivetran.com"
  },
  "extensionDependencies": [
    "ms-python.python"
  ],
  "engines": {
    "vscode": "^1.61.0"
  },
  "activationEvents": [
    "workspaceContains:**/dbt_project.yml"
  ],
  "main": "./client/out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "dbt Wizard",
      "properties": {
        "dbtWizard.dbtCompiler": {
          "type": "string",
          "order": 1,
          "enum": [
            "Auto",
            "dbt-rpc",
            "dbt"
          ],
          "default": "Auto",
          "description": "Command used to compile models."
        },
        "dbtWizard.trace.server": {
          "scope": "window",
          "type": "string",
          "order": 2,
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    },
    "commands": [
      {
        "command": "dbtWizard.showQueryPreview",
        "title": "Show query preview",
        "category": "dbtWizard",
        "icon": "$(book)"
      },
      {
        "command": "dbtWizard.compile",
        "title": "dbt compile",
        "category": "dbtWizard"
      },
      {
        "command": "dbtWizard.installLatestDbt",
        "title": "Install Latest dbt",
        "category": "dbtWizard"
      },
      {
        "command": "dbtWizard.installDbtAdapters",
        "title": "Install dbt Adapters",
        "category": "dbtWizard"
      },
      {
        "command": "dbtWizard.installDbtPackages",
        "title": "Install dbt Packages",
        "category": "dbtWizard"
      },
      {
        "command": "dbtWizard.restart",
        "title": "Restart dbtWizard",
        "category": "dbtWizard"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "dbtWizard.showQueryPreview",
          "group": "navigation",
          "when": "editorLangId =~ /^sql$|^jinja-sql$|^sql-bigquery$/ && !editorReadonly"
        }
      ],
      "dbt": [
        {
          "command": "dbtWizard.compile",
          "group": "1_common"
        },
        {
          "command": "dbtWizard.showQueryPreview",
          "group": "1_common"
        }
      ],
      "editor/context": [
        {
          "submenu": "dbt",
          "group": "navigation",
          "when": "editorLangId =~ /^sql$|^jinja-sql$|^sql-bigquery$/ && !editorReadonly"
        }
      ]
    },
    "submenus": [
      {
        "id": "dbt",
        "label": "dbt Wizard"
      }
    ]
  },
  "aiKey": "0a32611a-7944-4c4d-93c9-5c260a61395b",
  "size-limit": [
    {
      "path": "client/out/**/*"
    },
    {
      "path": "server/out/**/*.js"
    }
  ],
  "scripts": {
    "vscode:prepublish": "npm run webpack && ./prepare_node_modules.sh",
    "build": "tsc -b",
    "build:size": "npm run build && npm run vscode:prepublish",
    "watch": "tsc -b -w",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ../markdown_parser && npm install && cd ../e2e && npm install && cd ..",
    "lint": "eslint . --ext .ts",
    "lint-config": "eslint --print-config .eslintrc.json",
    "lint-fix": "eslint . --ext .ts --fix",
    "test": "mocha",
    "clean": "rm -rf common/out client/out server/out e2e/out",
    "clean-node-modules": "rm -rf node_modules && rm -rf client/node_modules && rm -rf server/node_modules",
    "clean-install": "npm run clean && npm run clean-node-modules && npm ci",
    "prepare-zetasql": "cp -R ./server/node_modules/@fivetrandevelopers/zetasql/lib/protos/ ./server/out/protos/ && cp -R ./server/node_modules/@fivetrandevelopers/zetasql/lib/zetasql/ ./server/out/zetasql/",
    "webpack": "npm run clean && tsc --project common/tsconfig.json && webpack --mode production --config ./client/webpack.config.js && webpack --mode production --config ./server/webpack.config.js && npm run prepare-zetasql",
    "webpack:dev": "npm run clean && webpack --mode none --config ./client/webpack.config.js && webpack --mode none --config ./server/webpack.config.js && npm run prepare-zetasql",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@size-limit/file": "^8.1.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "merge-options": "^3.0.4",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "size-limit": "^8.0.0",
    "ts-loader": "^9.4.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache",
      "prettier --write"
    ]
  }
}
