// Place your settings in this file to overwrite default and user settings.
{
  "typescript.preferences.quoteStyle": "single",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 2,

  "typescript.tsc.autoDetect": "off",

  "files.eol": "\n",
  "files.insertFinalNewline": true,

  "mochaExplorer.files": ["server/out/test/**/*.js", "client/out/test/**/*.js"],
  "mochaExplorer.require": "source-map-support/register",
  "mochaExplorer.watch": ["server/out/**/*.js", "client/out/**/*.js"],

  "cSpell.words": [
    "bigdecimal",
    "bignumeric",
    "bigserial",
    "clickhouse",
    "clipboardy",
    "countif",
    "covar_pop",
    "covar_samp",
    "cume_dist",
    "databricks",
    "datetime",
    "dbname",
    "deps",
    "devcontainers",
    "dremio",
    "duckdb",
    "dylib",
    "endcall",
    "enddocs",
    "endmacro",
    "eqeqeq",
    "exasol",
    "firebolt",
    "Fivetran",
    "fivetrandevelopers",
    "gcloud",
    "greenplum",
    "gserviceaccount",
    "hamjest",
    "ibmdb",
    "ifnull",
    "iname",
    "initcap",
    "iomete",
    "jinjahtml",
    "jinjas",
    "jsonpath",
    "keepalives",
    "keyfile",
    "libremote",
    "lpad",
    "ltrim",
    "millis",
    "mindsdb",
    "mkdir",
    "mockito",
    "neverthrow",
    "no-cond-assign",
    "nodir",
    "normalize_and_casefold",
    "ntile",
    "nulliferror",
    "ovsx",
    "Parens",
    "PARTITIONDATE",
    "PARTITIONTIME",
    "Pmfy",
    "promisified",
    "protos",
    "Pseudoterminal",
    "publicsuffix",
    "pyenv",
    "rockset",
    "rpad",
    "rtrim",
    "samuelcolvin",
    "setblock",
    "singlestore",
    "softbreak",
    "sonarjs",
    "sqlserver",
    "strpos",
    "tablesample",
    "teradata",
    "tidb",
    "trino",
    "uinteger",
    "unescapes",
    "unnest",
    "unpivot",
    "upsert",
    "venv",
    "vertica",
    "vsix",
    "werkzeug",
    "Xvfb",
    "zetasql"
  ],
  "cSpell.language": "en,en-GB",
  "files.autoSave": "afterDelay",

  "explorer.autoRevealExclude": {
    "**/node_modules": false
  },
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": [
      ".github/workflows/*.{yml,yaml}"
    ]
  }
}
